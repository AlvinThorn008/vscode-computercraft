{
  "name": "vscode-computercraft",
  "displayName": "ComputerCraft",
  "description": "Autocompletion for the ComputerCraft APIs (BETA)",
  "version": "0.0.1",
  "publisher": "JackMacWindows",
  "repository": {
    "type": "git",
    "url": "https://github.com/MCJack123/vscode-computercraft.git"
  },
  "private": true,
  "license": "MIT",
  "engines": {
    "vscode": "^1.20.0"
  },
  "categories": [
    "Other",
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:lua"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "languages": [
      {
        "id": "lua",
        "extensions": [
          ".lua"
        ],
        "aliases": [
          "lua"
        ]
      }
    ]
  },
  "icon": "images/icon.png",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/node": "^6.0.40",
    "fs": "0.0.1-security",
    "typescript": "^2.0.3",
    "typings": "^2.1.1",
    "vscode": "^1.1.22"
  }
}
