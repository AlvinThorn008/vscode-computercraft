{
  "name": "vscode-lua4rc",
  "displayName": "Lua4RC",
  "description": "Autocompletion for the Lua4RC API",
  "version": "0.1.0",
  "publisher": "Michel Jansson",
  "repository": {
    "type": "git",
    "url": "https://github.com/MichelJansson/vscode-lua4rc.git"
  },
  "private": true,
  "license": "MIT",
  "engines": {
    "vscode": "^1.20.0"
  },
  "categories": [
    "Other",
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:lua"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "languages": [
      {
        "id": "lua",
        "extensions": [
          ".lua"
        ],
        "aliases": [
          "lua"
        ]
      }
    ]
  },
  "icon": "images/icon.png",
  "scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
  "devDependencies": {
    "@types/node": "^6.0.40",
    "typescript": "^2.0.3",
    "vscode": "^1.0.0"
  }
}
